{
  "name": "jqueryPluginify",
  "version": "1.0.0",
  "author": "Ali Tavakoli",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.3.0",
    "browserify": "^17.0.0",
    "uglify-js": "^3.12.1"
  },
  "scripts": {
    "js-compile": "browserify -t [ babelify --presets [ es2015 ] ] app/_core/bundles/jp-all.js -o dist/scripts/jp-all.js",
    "js-minify": "uglifyjs dist/scripts/jp-all.js -o dist/scripts/jp-all.min.js",
    "build": "npm run js-compile && npm run js-minify"
  }
}
